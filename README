  /usr/share/doc/kopenbabel-0.5/ChangeLog
  /usr/share/doc/kopenbabel-0.5/COPYING
  /usr/share/doc/kopenbabel-0.5/INSTALL
  /usr/share/doc/kopenbabel-0.5/NEWS
  /usr/share/doc/kopenbabel-0.5/README
  /usr/share/kopenbabel/kopenbabelui.rc
  /usr/share/locale/de/LC_MESSAGES/kopenbabel.mo
  /usr/share/locale/fr/LC_MESSAGES/kopenbabel.mo

